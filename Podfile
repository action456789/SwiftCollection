    pod 'Alamofire', '~> 4.4'
    pod 'AlamofireNetworkActivityIndicator', '~> 2.0'
    
    # 简化 JSON 访问
    pod 'SwiftyJSON'
    
    # JSON 转 对象
    # pod 'ObjectMapper', '~> 1.3'
    
    # swift 版 Masonry
    pod 'SnapKit'
    
    pod 'Charts'
    pod 'DeviceKit', '~> 1.0'
    pod 'ReactiveCocoa', '~> 5.0.0'
    pod 'RxSwift'
    
    # 最小子序列
    pod 'SwiftLCS'
    pod 'Spring', :git => 'https://github.com/MengTo/Spring.git', :branch => 'swift4'
end

